<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.quantumsoft.koreasurvey.FirebasetokenMapper">
<insert id="insertToken" parameterType="kr.quantumsoft.koreasurvey.model.Firebasetoken">
	INSERT INTO `firebasetoken` (
		`userid`,
		`token`,
		`sessionid`
	) VALUES (
		#{userid},
		#{token},
		#{sessionid}
	)
</insert>
</mapper>